<?xml version="1.0" encoding="UTF-8"?>
<portlet-app 
    xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd 
                        /opt/SUNWportal/dtd/portlet.xsd"
    version="1.0">

  <portlet>
    <portlet-name>Module</portlet-name>
    <portlet-class>ca.uoguelph.ccs.portal.modules.ModulePortlet</portlet-class>
    <expiration-cache>30</expiration-cache>
    <supports>
      <mime-type>text/html</mime-type>
      <portlet-mode>view</portlet-mode>
      <portlet-mode>edit</portlet-mode>
      <portlet-mode>help</portlet-mode>
    </supports>
    <supported-locale>en_US</supported-locale>
    <portlet-info>
      <title>Module Portlet</title>
      <short-title>Module</short-title>
      <keywords>module</keywords>
    </portlet-info> 
  </portlet>   

  <portlet>
    <portlet-name>MyCourses</portlet-name>
    <portlet-class>ca.uoguelph.ccs.portal.modules.ModulePortlet</portlet-class>
    <expiration-cache>30</expiration-cache>
    <supports>
      <mime-type>text/html</mime-type>
      <portlet-mode>view</portlet-mode>
      <portlet-mode>edit</portlet-mode>
      <portlet-mode>help</portlet-mode>
    </supports>
    <supported-locale>en_US</supported-locale>
    <portlet-info>
      <title>MyCourses Portlet</title>
      <short-title>MyCourses</short-title>
      <keywords>course courses</keywords>
    </portlet-info> 
  </portlet>

  <portlet>
    <portlet-name>WEBMail</portlet-name>
    <portlet-class>ca.uoguelph.ccs.portal.modules.ModulePortlet</portlet-class>
    <expiration-cache>30</expiration-cache>
    <supports>
      <mime-type>text/html</mime-type>
      <portlet-mode>view</portlet-mode>
      <portlet-mode>edit</portlet-mode>
      <portlet-mode>help</portlet-mode>
    </supports>
    <supported-locale>en_US</supported-locale>
    <portlet-info>
      <title>WEBMail Portlet</title>
      <short-title>WebMail</short-title>
      <keywords>webmail mail email</keywords>
    </portlet-info>
  </portlet>

  <portlet>
    <portlet-name>TestScoring</portlet-name>
    <portlet-class>ca.uoguelph.ccs.portal.modules.ModulePortlet</portlet-class>
    <expiration-cache>30</expiration-cache>
    <supports>
      <mime-type>text/html</mime-type>
      <portlet-mode>view</portlet-mode>
      <portlet-mode>edit</portlet-mode>
      <portlet-mode>help</portlet-mode>
    </supports>
    <supported-locale>en_US</supported-locale>
    <portlet-info>
      <title>Test Scoring</title>
      <short-title>Test Scoring</short-title>
      <keywords>test scoring scanner</keywords>
    </portlet-info>
  </portlet>

  <portlet>
    <portlet-name>OracleCalendar</portlet-name>
    <portlet-class>ca.uoguelph.ccs.portal.modules.ModulePortlet</portlet-class>
    <expiration-cache>30</expiration-cache>
    <supports>
      <mime-type>text/html</mime-type>
      <portlet-mode>view</portlet-mode>
      <portlet-mode>edit</portlet-mode>
      <portlet-mode>help</portlet-mode>
    </supports>
    <supported-locale>en_US</supported-locale>
    <portlet-info>
      <title>Oracle Calendar</title>
      <short-title>OCal</short-title>
      <keywords>oracle calendar schedule agenda</keywords>
    </portlet-info>
  </portlet>

